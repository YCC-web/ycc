{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "module": "ESNext",
    "types": ["vite/client"],
    
    "sourceMap": true,
    "resolveJsonModule": true,

    "noImplicitReturns": true
  },
  "exclude": ["node_modules"]
}